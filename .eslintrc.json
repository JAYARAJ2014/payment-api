{
    "extends": [
        "eslint:recommended", 
        "plugin:@typescript-eslint/recommended",
        "prettier"
      ],
  
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType":"module"
    },
    "plugins": [
      "typescript-eslint",
      "prettier"
    ],
    "settings": {
      "import/resolver":{
        "typescript":{}
      }
    }
  }